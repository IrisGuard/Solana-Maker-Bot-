# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Solana RPC Endpoints (JSON array of strings or comma-separated list)
EXPO_PUBLIC_SOLANA_RPC_ENDPOINTS=["https://api.mainnet-beta.solana.com","https://solana-api.projectserum.com","https://rpc.ankr.com/solana","https://solana.public-rpc.com"]

# API Keys (can be JSON arrays or comma-separated lists for multiple keys)
EXPO_PUBLIC_JUPITER_API_KEY=your-jupiter-api-key-1,your-jupiter-api-key-2
EXPO_PUBLIC_COINGECKO_API_KEY=key1,key2,key3,key4,key5

# Feature Flags
EXPO_PUBLIC_ENABLE_REAL_TRANSACTIONS=false
EXPO_PUBLIC_ENABLE_AUTO_BOOST=true
EXPO_PUBLIC_SIMULATION_MODE=true

# Performance Settings
EXPO_PUBLIC_MAX_CONCURRENT_MAKERS=100
EXPO_PUBLIC_API_TIMEOUT_MS=5000
EXPO_PUBLIC_TRANSACTION_BATCH_SIZE=20

# UI Configuration
EXPO_PUBLIC_DEFAULT_THEME=dark
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_APP_NAME=Solana Maker Bot

# Backend Configuration
EXPO_PUBLIC_RORK_API_BASE_URL=http://localhost:3000